<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>

    <script>
    var names = ['Amy','Bob','Cindy']
        function checkName() {
            var y = document.getElementById("alert")
            y.innerHTML = ''
            for (var i = 0; i < names.length; i++) {
                if (document.getElementById("name").value === names[i]) {
                    y.innerHTML = '用户名重复';
                }
            }
        }
        function checkLeave() {
            if (document.getElementById("name").value || document.getElementById("pwd").value) {
                event.returnValue = "确定离开当前页面吗？";
            }
  　　}

    </script>
</head>
<body onbeforeunload="checkLeave()">
    @RenderBody()
    用户名：<input id="name" type="text" onchange="checkName()" />
    <br />
    <p id="alert"></p>
    密码：<input id="pwd" type="text" />
    <br />

</body>
</html>
